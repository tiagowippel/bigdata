{
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 5,
    "editor.fontFamily": "'JetBrains Mono', 'Operator Mono', 'Dank Mono', 'Fira Code', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 17,
    "editor.fontWeight": "400",
    "editor.formatOnSave": true,
    "editor.letterSpacing": 0.5,
    "editor.lineHeight": 25,
    "editor.mouseWheelZoom": true,
    "editor.renderWhitespace": "all",
    "editor.suggestSelection": "first",
    "files.trimTrailingWhitespace": true,
    "javascript.implicitProjectConfig.experimentalDecorators": true,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "es5",
    "vsicons.dontShowNewVersionMessage": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "bookmarks.saveBookmarksInProject": true,
    "numberedBookmarks.saveBookmarksInProject": true,
    "git.ignoreLimitWarning": true,
    "editor.foldingStrategy": "indentation",
    "githd.blameView.enabled": false,
    "indentRainbow.ignoreErrorLanguages": [
        "*"
    ],
    "workbench.colorCustomizations": {
        "activityBar.background": "#50b36e",
        "activityBar.activeBorder": "#ede9f5",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#ede9f5",
        "activityBarBadge.foreground": "#15202b",
        "titleBar.activeBackground": "#3f9158",
        "titleBar.inactiveBackground": "#3f915899",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveForeground": "#e7e7e799",
        "statusBar.background": "#3f9158",
        "statusBarItem.hoverBackground": "#50b36e",
        "statusBar.foreground": "#e7e7e7"
    },
    "peacock.color": "#3f9158",
    "javascript.validate.enable": false,
    "typescript.validate.enable": false
}
